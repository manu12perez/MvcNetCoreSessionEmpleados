@model List<Empleado>

<h1 style="color:darkblue">Session Empleados List</h1>

<p>
	<a asp-controller="Empleados"
	   asp-action="EmpleadosAlmacenados"
	   class="btn btn-success">
		Empleados Almacenados
	</a>
</p>

<h4>@ViewData["MENSAJE"]</h4>

<ul class="list-group list-group-item-warning">
	@foreach (Empleado empleado in Model)
	{
		<li class="list-group-item">
			@empleado.Apellido
			<a asp-controller="Empleados"
			   asp-action="SessionEmpleados"
			   asp-route-idempleado="@empleado.IdEmpleado">
				Almacenar empleado: @empleado.IdEmpleado
			</a>
		</li>
	}
</ul>